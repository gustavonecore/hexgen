{
	"name": "gustavonecore/hexgen",
	"description": "Command and handler generator",
	"license": "proprietary",

	"require": {
		"league/tactician": "~0.6",
		"twig/twig": "^1.24",
		"twig/extensions": "^1.3"
	},

	"require-dev": {
		"phpunit/phpunit": "^4.6",
		"phpunit/dbunit": "^1.3",
		"phpunit/php-invoker": "^1.1"
	},

	"autoload": {
		"psr-4": {
			"Hexgen\\": "src/",
			"Test\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},

	"minimum-stability": "dev",
	"prefer-stable": true
}
